        },
        {
          "body": "airbending",
          "key": "${link_1}",
          "target": {
            "external": false,
            "href": "/wiki/Airbending",
            "page": "Airbending"
          }
        },
        {
          "body": "Earth Queen",
          "key": "${link_2}",
          "target": {
            "external": false,
            "href": "/wiki/Earth_monarch",
            "page": "Earth_monarch"
          }
        },
        {
          "body": "army",
          "key": "${link_3}",
          "target": {
